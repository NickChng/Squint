<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema elementFormDefault="qualified" attributeFormDefault="qualified" version="1.2" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!-- 		
			Filename:			ProtocolTypes.xsd
			Copyright, Varian Medical systems, Inc., 2004 - 2005
			All Rights Reserved
