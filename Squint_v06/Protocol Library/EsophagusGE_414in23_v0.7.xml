          </BolusDefinitions>
          <ValidGeometries>
            <Geometry GeometryName ="Full Arc (CCW)" StartAngle="179" StartAngleTolerance="1" EndAngle="181" EndAngleTolerance="1" Trajectory="CCW"/>
          </ValidGeometries>
          <ValidEnergies>
            <Energy Mode="6X"/>
