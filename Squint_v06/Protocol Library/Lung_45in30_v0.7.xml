<?xml version="1.0" encoding="utf-8"?>
<SquintProtocol xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <version>1</version>
  <ProtocolMetaData 
    ProtocolName="Lung (45 Gy in 30#)" ProtocolType="Clinical" NumComponents="1" ProtocolDate="03-08-2017" Intent="Curative" ApprovalStatus ="Approved" DiseaseSite="Lung" TreatmentCentre="CN"/>

  <!-- Defines target structures and assigns associated aliases for each one. Also include checks for stray voxels as necessary.-->
  <Structures>
    <Structure Label="CTV High Risk" ProtocolStructureName="CTV High">
      <StructureChecklist>
        <PointContourCheck Threshold="0.05"/>
      </StructureChecklist>
      <EclipseAliases>
        <EclipseId Id="CTVp_45"></EclipseId>
        <EclipseId Id="CTV_45"></EclipseId>
        <EclipseId Id="CTV45"></EclipseId>
        <EclipseId Id="CTV"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="GTV Primary" ProtocolStructureName="GTV Primary">
      <StructureChecklist>
        <PointContourCheck Threshold="0.05"/>
      </StructureChecklist>
      <EclipseAliases>
        <EclipseId Id="GTV"></EclipseId>
        <EclipseId Id="GTVp"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="GTV Nodal" ProtocolStructureName="GTV Nodal">
      <StructureChecklist>
        <PointContourCheck Threshold="0.05"/>
      </StructureChecklist>
      <EclipseAliases>
        <EclipseId Id="GTVn"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="PTV High Risk" ProtocolStructureName="PTV High Dose">
      <EclipseAliases>
        <EclipseId Id="PTV_45e"></EclipseId>
        <EclipseId Id="PTVp_45e"></EclipseId>
        <EclipseId Id="PTV_45"></EclipseId>
        <EclipseId Id="PTVp_45"></EclipseId>
        <EclipseId Id="PTV_45"></EclipseId>
        <EclipseId Id="PTV45"></EclipseId>
      </EclipseAliases>
    </Structure>

    <!--Defines OAR structures and assigns acceptable aliases for each OAR.-->
    <Structure Label="Segment of spinal cord" ProtocolStructureName="SpinalCord_PRV">
      <EclipseAliases>
        <EclipseId Id="SpinalCord_PRV"></EclipseId>
        <EclipseId Id="SpinalCordPRV"></EclipseId>
        <EclipseId Id="Cord_PRV"></EclipseId>
        <EclipseId Id="CordPRV"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="Lungs sub GTVs" ProtocolStructureName="Lungs_sub_IGTV">
      <EclipseAliases>
        <EclipseId Id="Lungs_sub_IGTV"></EclipseId>
        <EclipseId Id="Lungs_sub_GTV"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="Heart" ProtocolStructureName="Heart">
      <EclipseAliases>
        <EclipseId Id="Heart"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="Esophagus" ProtocolStructureName="Esophagus">
      <EclipseAliases>
        <EclipseId Id="Esophagus"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="Brachial Nerve Plexus Left" ProtocolStructureName="BrachialPlexus (Left)">
      <EclipseAliases>
        <EclipseId Id="BrachialPlexus_L"></EclipseId>
        <EclipseId Id="BrachialPlex_L"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="Brachial Nerve Plexus Right" ProtocolStructureName="BrachialPlexus (Right)">
      <EclipseAliases>
        <EclipseId Id="BrachialPlexus_R"></EclipseId>
        <EclipseId Id="BrachialPlex_R"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="Liver" ProtocolStructureName="Liver">
      <EclipseAliases>
        <EclipseId Id="Liver"></EclipseId>
      </EclipseAliases>
    </Structure>
    <Structure Label="Control" ProtocolStructureName="Artifacts">
      <EclipseAliases>
        <EclipseId Id="Artifacts"></EclipseId>
        <EclipseId Id="Artifact"></EclipseId>
      </EclipseAliases>
    </Structure>
  </Structures>

  <!--This section defines default parameters for plan checklists including Artifacts, Supports, Calculation algorithms, simulations and prescription values-->
  <ProtocolChecklist>
    <Artifacts>
      <Artifact HU="0" Tolerance ="0.1" ProtocolStructureName="Artifacts"/>
    </Artifacts>
    <Supports CouchSurface="-300" CouchInterior="-1000"/>
    <Calculation Algorithm="AAA_13623" AlgorithmResolution="2.5" HeterogeneityOn="true" FieldNormalizationMode="ISO"/>
    <Simulation SliceSpacing ="2.5"/>
  </ProtocolChecklist>

  <!--This section defines the Components used in plan checking including Imaging protocols, beams. Each phase must have its own component element-->
  <Components>
    <Component ComponentName="Single Phase" Type="Phase" >
      <Prescription PNVMin="97" PNVMax="103" PrescribedPercentage="100" NumFractions="30" ReferenceDose="4500"/>
      <ImagingProtocols>
        <ImagingProtocol Id="kv_2D" />
        <ImagingProtocol Id="PostCBCT" />
      </ImagingProtocols>
      <!-- This section defines beam child elements including Eclipse aliases, valid geometries, energies and bolus definitions. Each arc in a plan must have its own Beam element. -->
      <Beams MinBeams="1" MaxBeams="2" NumIso="1" MinColOffset="20">
        <Beam ProtocolBeamName="ARC CCW" Technique="ARC" StartAngle="179" EndAngle="181" ToleranceTable="CN+VI Indexed" MinColRotation="20" MaxColRotation="60" CouchRotation="0" MinX="3" MaxX="18" MinY="3" MaxY="20" JawTracking_Indication="Required">
          <EclipseAliases>
            <EclipseId Id="rarc_ccw"/>
            <EclipseId Id="arc_ccw"/>
          </EclipseAliases>
          <ValidGeometries>
            <Geometry GeometryName = "Right Partial Arc (CCW)" StartAngle="15" StartAngleTolerance="15" EndAngle="181" EndAngleTolerance="1" Trajectory="CCW"/>
            <Geometry GeometryName = "Left Partial Arc (CCW)" StartAngle="179" StartAngleTolerance="1" EndAngle="345" EndAngleTolerance="15" Trajectory="CCW"/>
          </ValidGeometries>
          <ValidEnergies>
            <Energy Mode="6X"/>
            <Energy Mode="10XFFF"/>
          </ValidEnergies>
          <BolusDefinitions>
            <Bolus Indication="None"/>
          </BolusDefinitions>
        </Beam>
        <Beam ProtocolBeamName="ARC CW" Technique="ARC" StartAngle="181" EndAngle="179" ToleranceTable="CN+VI Indexed" MinColRotation="20" MaxColRotation="60" CouchRotation="0" MinX="3" MaxX="18" MinY="3" MaxY="20" JawTracking_Indication="Required">
          <EclipseAliases>
            <EclipseId Id="rarc_cw"/>
            <EclipseId Id="arc_cw"/>
          </EclipseAliases>
          <ValidGeometries>
            <Geometry GeometryName="Left Partial Arc (CW)" StartAngle="345" StartAngleTolerance="15" EndAngle="179" EndAngleTolerance="1" Trajectory="CW"/>
            <Geometry GeometryName="Right Partial Arc (CW)" StartAngle="181" StartAngleTolerance="1" EndAngle="15" EndAngleTolerance="15" Trajectory="CW"/>
          </ValidGeometries>
          <ValidEnergies>
            <Energy Mode="6X"/>
            <Energy Mode="10XFFF"/>
          </ValidEnergies>
          <BolusDefinitions>
            <Bolus Indication="None"/>
          </BolusDefinitions>
        </Beam>
      </Beams>
    </Component>
  </Components>

  <!--This section defines the Constraints (DVH, conformity index etc) as based on clinical protocols-->
  <Constraints>
    <Constraint ComponentName="Single Phase" ProtocolStructureName="SpinalCord_PRV" ConstraintType="D" ConstraintValue="0.035" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="4800" ReferenceUnit="absolute" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="SpinalCord_PRV" ConstraintType="V" ConstraintValue="4200" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="2" MinorViolation="1" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Lungs_sub_IGTV" ConstraintType="M" ConstraintValue="1700" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="2000" MinorViolation="1700" ReferenceUnit="absolute" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Lungs_sub_IGTV" ConstraintType="V" ConstraintValue="2000" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="35" MinorViolation="30" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Lungs_sub_IGTV" ConstraintType="V" ConstraintValue="500" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="70" MinorViolation="60" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="PTV High Dose" ConstraintType="V" ConstraintValue="95" ConstraintUnit="relative" ReferenceType="lower" MajorViolation="98" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="PTV High Dose" ConstraintType="V" ConstraintValue="93" ConstraintUnit="relative" ReferenceType="lower" MajorViolation="99" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="PTV High Dose" ConstraintType="D" ConstraintValue="0" ConstraintUnit="relative" ReferenceType="upper" MajorViolation="110" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="CTV High" ConstraintType="D" ConstraintValue="100" ConstraintUnit="relative" ReferenceType="lower" MajorViolation="95" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Heart" ConstraintType="D" ConstraintValue="0" ConstraintUnit="relative" ReferenceType="upper" MajorViolation="105" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Heart" ConstraintType="M" ConstraintValue="2600" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="2600" ReferenceUnit="absolute" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Heart" ConstraintType="V" ConstraintValue="3000" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="40" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Esophagus" ConstraintType="D" ConstraintValue="0" ConstraintUnit="relative" ReferenceType="upper" MajorViolation="105" ReferenceUnit="relative" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Esophagus" ConstraintType="M" ConstraintValue="3400" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="3400" ReferenceUnit="absolute" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="BrachialPlexus (Left)" ConstraintType="D" ConstraintValue="0" ConstraintUnit="relative" ReferenceType="upper" MajorViolation="6300" MinorViolation="6000" ReferenceUnit="absolute" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="BrachialPlexus (Right)" ConstraintType="D" ConstraintValue="0" ConstraintUnit="relative" ReferenceType="upper" MajorViolation="6300" MinorViolation="6000" ReferenceUnit="absolute" />
    <Constraint ComponentName="Single Phase" ProtocolStructureName="Liver" ConstraintType="M" ConstraintValue="2300" ConstraintUnit="absolute" ReferenceType="upper" MajorViolation="2300" ReferenceUnit="absolute" />
  </Constraints>
</SquintProtocol>
