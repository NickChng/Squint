            <EclipseId Id= "arc_cw"/>
          </EclipseAliases>
          <ValidGeometries>
            <Geometry GeometryName ="Full Arc (CW)" StartAngle="181" StartAngleTolerance="2" EndAngle="179" EndAngleTolerance="2" Trajectory="CW"/>
          </ValidGeometries>
          <ValidEnergies>
